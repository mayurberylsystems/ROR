<h1>Doctor details form</h1> 

<%= form_for :doctor, url: doctors_path do |f| %>

<p>
<%= f.label :name %><br>
<%= f.text_field :name %>
</p>
<p>
<%= f.label :age %><br>
<%= f.text_field :age %>
</p>
</p>
<%= f.label :email %><br>
<%= f.text_field :email %>
</p>
</p>
<%= f.label :address %><br>
<%= f.text_field :address %>
</p>
<p>
<%= f.submit "submit"%>
</p>
<%= link_to 'Doctors\' List', doctors_path%>
<% end %>






