<h3> All user </h3>
<table>
	<tr>
		<th>  User Name </th>
		<th> Email </th>
	</tr>
	<% @users.each do |c| %>
		<tr>
			<td>
				<%= c.name %>
			</td>
			<td>
				<%= c.email %>
			</td>
			<td>
				<%= link_to 'Edit', "/user/#{c.id}/edit" %>
			</td>
			<td>
			<%= link_to 'Delete', "/user/#{c.id}", method: :delete, data: {confirm: 'ARE YOU SURE??'} %>
			</td>
		</tr>	
	<% end %>
</table>